        "PropertiesWindow": {
            "name": "检视面板",
            "type": "button",
            "class": "PropertiesWindow",
            "btnUp": "closeWindow",
            "btnDown": "openWindow",
            "image_url": "./menuGUI/img/propWindow.png",
            "shortcut": [
                "P",
                "p"
            ]
        },
        "HierarchyWindow": {
            "name": "层级面板",
            "type": "button",
            "class": "HierarchyWindow",
            "btnUp": "closePanel",
            "btnDown": "openPanel",
            "image_url": "./menuGUI/img/hierarchyWindow.png",
            "shortcut": [
                "H",
                "h"
            ]
        },
        "FullScreen": {
            "name": "全屏显示",
            "type": "button",
            "class": "FullScreen",
            "btnUp": "exitFullScreen",
            "btnDown": "requestFullScreen",
            "image_url": "./menuGUI/img/fullScreen.png",
            "shortcut": [
                "F11"
            ]
        }
    },
    "EditorMode": {
        "Title": "编辑模式",
        "SelectTool": {
            "name": "选择工具",
            "type": "button",
