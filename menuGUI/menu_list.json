    "EditorMode": {
        "Title": "编辑模式",
        "SelectTool": {
            "name": "选择工具",
            "type": "button",
            "class": "SelectionTool",
            "param": {},
            "btnUp": "selectionUp",
            "btnDown": "selectionDown",
            "image_url": "./menuGUI/img/selectTool.png",
            "shortcut": [
                "Q",
                "q"
            ]
        },
        "MoveTool": {
            "name": "移动工具",
            "type": "radio",
            "class": "TransformBySelection",
            "image_url": "./menuGUI/img/moveTool.png",
            "shortcut": [
                "W",
                "w"
            ]
        },
        "RotateTool": {
            "name": "旋转工具",
            "type": "radio",
            "class": "TransformBySelection",
            "image_url": "./menuGUI/img/rotateTool.png",
            "shortcut": [
                "E",
                "e"
            ]
        },
        "ScaleTool": {
            "name": "缩放工具",
            "type": "radio",
            "class": "TransformBySelection",
            "image_url": "./menuGUI/img/scaleTool.png",
            "shortcut": [
                "R",
                "r"
            ]
        },
        "WholeTool": {
            "name": "整体或单独工具",
            "type": "button",
            "class": "TransformWhole",
            "btnUp": "independent",
            "btnDown": "whole",
            "image_url": "./menuGUI/img/selectionWhole.png",
            "shortcut": [
                "A",
                "a"
            ]
        }
    },
